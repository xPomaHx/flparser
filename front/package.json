{
  "name": "flparser",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "xPomaHx <gigoroma@yandex.ru>",
  "scripts": {
    "dev": "nuxt src/",
    "build": "nuxt build src/",
    "start": "nuxt start src/",
    "generate": "nuxt generate src/",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "license": "ISC",
  "dependencies": {
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^4.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-standard": "*",
    "eslint-config-prettier-standard": "*"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "prettier",
      "prettier-standard"
    ],
    "plugins": [
      "import",
      "prettier",
      "standard"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "parser": "babel-eslint"
    },
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-console": "off",
      "space-before-function-paren": 0,
      "new-cap": 0,
      "prettier/prettier": 2,
      "no-control-regex": "off"
    }
  }
}
