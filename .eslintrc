{
  "extends": ["standard", "prettier", "prettier/standard"],
  "plugins": ["import", "prettier", "standard"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "parser": "babel-eslint"
  },
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "no-console": "off",
    "space-before-function-paren": 0,
    "new-cap": 0,
    "prettier/prettier": 2,
    "no-control-regex": "off"
  }
}
